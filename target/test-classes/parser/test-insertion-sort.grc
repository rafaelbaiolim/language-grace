var arr[5]:int;
def insertionSort(x[]:int; n:int){
   var i, key, j:int;
   for (i = 1; i < n; i+=1){
       key = x[i];
       j = i - 1;

       while (j >= 0 && x[j] > key){
           x[j+1] = x[j];
           j = j-1;
       }
       x[j+1] = key;
   }
}

def printArray(y[]:int; n:int)
{
   var i:int;
   for (i=0; i < n; i+=1){
       write y[i], " ";
   }
   write "\n";
}

arr[0] = 12;
arr[1] = 11;
arr[2] = 13;
arr[3] = 5;
arr[4] = 6;

write "inicial:";
printArray(arr, 5);
write "::";

insertionSort(arr, 5);
write "ordenado:";
printArray(arr, 5);
